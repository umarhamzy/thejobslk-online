<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>AppointmentController</servlet-name>
  	<display-name>AppointmentController</display-name>
  	<description></description>
  	<servlet-class>com.thejobslk.controller.AppointmentController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ConsultantController</servlet-name>
  	<display-name>ConsultantController</display-name>
  	<description></description>
  	<servlet-class>com.thejobslk.controller.ConsultantController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>JobseekerController</servlet-name>
  	<display-name>JobseekerController</display-name>
  	<description></description>
  	<servlet-class>com.thejobslk.controller.JobseekerController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ManagerController</servlet-name>
  	<display-name>ManagerController</display-name>
  	<description></description>
  	<servlet-class>com.thejobslk.controller.ManagerController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>AppointmentController</servlet-name>
  	<url-pattern>/appointmentmanager</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AppointmentController</servlet-name>
  	<url-pattern>/getappointment</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ConsultantController</servlet-name>
  	<url-pattern>/consultantmanager</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ConsultantController</servlet-name>
  	<url-pattern>/getconsultant</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>JobseekerController</servlet-name>
  	<url-pattern>/jobseekermanager</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>JobseekerController</servlet-name>
  	<url-pattern>/getjobseeker</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ManagerController</servlet-name>
  	<url-pattern>/ManagerController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ManagerController</servlet-name>
  	<url-pattern>/getmanager</url-pattern>
  </servlet-mapping>
</web-app>
